plot2_total <- function(dat){
# dat : dataset used to plot
p2_total <- ggplot(dat, aes(y = fct_reorder(Nation, Total, .desc = F), x = Total))+
geom_segment(aes(y = fct_reorder(Nation, Total, .desc = F),
yend = fct_reorder(Nation, Total, .desc = F),
x = 0,
xend = Total),
color = "cyan",
lwd = 2)+
geom_point(color = "cyan", size = 12, alpha=0.6, pch = 16)+
geom_text(aes(label = Total), color = 'black', size = 4)+
theme_light()+
theme(
plot.title = element_text(size = 30,
face = "bold",
color = 'grey28',
hjust = 0.5,
lineheight = 1.2),
axis.text = element_text(size = 15,
face = "bold"),
panel.grid.major.x = element_blank(),
axis.ticks.y = element_blank(),
panel.border = element_blank(),
axis.title = element_text(face = "bold",
size = 20)
)+
labs(x = "Number of Total Medals",
y = "Nation",
title = "Race For Medals")
p2_total
}
